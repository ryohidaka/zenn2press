{
  "name": "zenn2press",
  "description": "Convert Zenn content format to VitePress.",
  "version": "0.0.0",
  "author": "ryohidaka <39184410+ryohidaka@users.noreply.github.com>",
  "bin": {
    "zenn2press": "./bin/run.js"
  },
  "bugs": "https://github.com/ryohidaka/zenn2press/issues",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^20",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "husky": "^9.0.11",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3.2.5",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typedoc": "^0.25.13",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ryohidaka/zenn2press#readme",
  "keywords": [
    "oclif",
    "zenn",
    "vitepress"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "zenn2press",
    "dirname": "zenn2press",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "apps": {
        "description": "Convert Zenn content format to VitePress."
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryohidaka/zenn2press.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --fix --quiet",
    "prepare": "husky install",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "format": "prettier . --write",
    "docs": "typedoc"
  },
  "types": "dist/index.d.ts"
}
